
class Prison{
	
	field Guard amy;
	field Guard bob;
	field Guard carl;

	constructor Prison new(){

	let amy= Guard.new(50,50);
	let bob= Guard.new(80,80);
	let carl= Guard.new(100,100);
	return this;
	}

	method void dispose(){
	do amy.dispose();
	do bob.dispose();
	do carl.dispose();

	do Memory.deAlloc(this);
	return;
	}

}

